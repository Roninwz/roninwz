{
  "name": "@roninwz-ui/core",
  "version": "0.0.4",
  "description": "自定义适用于vue的完全自主封装的ui组件库",
  "author": "Roninwz",
  "private": false,
  "main": "lib/@roninwz-ui-core.umd.min.js",
  "scripts": {
    "serve": "vue-cli-service serve",
    "dev": "vue-cli-service serve",
    "build:lib": "vue-cli-service build --target lib --name @roninwz-ui-core --dest lib src/index.js",
    "build:theme": "gulp build --gulpfile src/style/gulpfile.js",
    "lint": "vue-cli-service lint",
    "firstlog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s"
  },
  "dependencies": {
    "cheerio": "^1.0.0-rc.10",
    "core-js": "^3.6.5",
    "vue": "^2.6.11",
    "vue-router": "^3.5.2"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^6.2.2",
    "extract-text-webpack-plugin": "^3.0.2",
    "gulp": "^4.0.0",
    "gulp-autoprefixer": "^6.0.0",
    "gulp-cssmin": "^0.2.0",
    "gulp-sass": "^4.0.2",
    "node-sass": "^7.0.0",
    "sass": "^1.35.2",
    "sass-loader": "^7.1.0",
    "svg-sprite-loader": "^6.0.9",
    "vue-template-compiler": "^2.6.11"
  },
  "homepage": "https://roninwz.github.io/roninwz-ui/",
  "repository": "https://github.com/roninwz/roninwz-ui",
  "keywords": [
    "vue",
    "ui库"
  ],
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {}
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "gitHead": "cca6256fa1d38bef3a62788d229921b745b926d3"
}
